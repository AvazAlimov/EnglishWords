<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<?import java.net.URL?>
<GridPane alignment="CENTER" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.76-ea"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Settings">
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
    </rowConstraints>
    <Button fx:id="addWord" id="addWordButton" onAction="#doAction" alignment="CENTER" text="Button"
            GridPane.halignment="CENTER"
            GridPane.valignment="CENTER"/>
    <Button fx:id="deleteWord" id="deleteWordButton" onAction="#doAction" GridPane.rowIndex="1" alignment="CENTER"
            text="Button"
            GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    <Button fx:id="editWord" id="editWordButton" onAction="#doAction" GridPane.rowIndex="2" alignment="CENTER"
            text="Button"
            GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    <Button fx:id="search" id="searchButton" onAction="#doAction" GridPane.rowIndex="3" alignment="CENTER"
            text="Search"
            GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    <Button fx:id="back" id="backButton" onAction="#doAction" GridPane.rowIndex="4" alignment="CENTER" text="Button"
            GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    <stylesheets>
        <URL value="@Style.css"/>
    </stylesheets>
</GridPane>
